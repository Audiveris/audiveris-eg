<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<filter>
    <differences>
        <difference name="number of child nodes">
            <optional>true</optional>
        </difference>
    </differences>
    <elements>
        <element name="*">
            <optional>true</optional>
            <attributes>
                <attribute name="default-x">
                    <tolerance-ratio>0.02</tolerance-ratio>
                </attribute>
                <attribute name="default-y">
                    <tolerance-ratio>0.02</tolerance-ratio>
                    <tolerance-delta>5.0</tolerance-delta>
                </attribute>
            </attributes>
        </element>
    </elements>
</filter>
