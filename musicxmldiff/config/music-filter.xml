<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<filter>
    
    <!-- Differences -->
    <differences>
        
        <difference name="number of child nodes">
            <optional>true</optional>
        </difference>
    
        <difference name="number of element attributes">
            <optional>true</optional>
        </difference>
        
    </differences>
    
    <!-- Elements -->
    <elements>
        
        <!-- Dummy element for global attributes -->
        <element name="*">
            <attributes>
                <attribute name="default-x">
                    <tolerance-ratio>0.1</tolerance-ratio>
                </attribute>
                <attribute name="default-y">
                    <tolerance-ratio>0.1</tolerance-ratio>
                </attribute>
                <attribute name="left-margin">
                    <tolerance-ratio>0.02</tolerance-ratio>
                </attribute>
                <attribute name="right-margin">
                    <tolerance-ratio>0.02</tolerance-ratio>
                </attribute>
                <attribute name="width">
                    <tolerance-ratio>0.02</tolerance-ratio>
                </attribute>
            </attributes>
        </element>
    
        <!-- Elements -->
        <element name="encoding">
            <optional>true</optional>
        </element>
    
        <element name="left-margin">
            <tolerance-ratio>0.02</tolerance-ratio>
            <tolerance-delta>5</tolerance-delta>
        </element>
    
        <element name="lyric">
            <attributes>
                <attribute name="justify">
                    <optional>true</optional>
                </attribute>
            </attributes>
        </element>
    
        <element name="millimeters">
            <tolerance-ratio>0.02</tolerance-ratio>
        </element>
    
        <element name="page-height">
            <tolerance-ratio>0.02</tolerance-ratio>
        </element>
    
        <element name="page-width">
            <tolerance-ratio>0.02</tolerance-ratio>
        </element>
    
        <element name="repeat">
            <attributes>
                <attribute name="winged">
                    <optional>true</optional>
                </attribute>
            </attributes>
        </element>
    
        <element name="right-margin">
            <tolerance-ratio>0.02</tolerance-ratio>
            <tolerance-delta>5</tolerance-delta>
        </element>
    
        <element name="score-partwise">
            <attributes>
                <attribute name="version">
                    <optional>true</optional>
                </attribute>
            </attributes>
        </element>
    
        <element name="slur">
            <attributes>
                <attribute name="bezier-x">
                    <optional>true</optional>
                </attribute>
                <attribute name="bezier-y">
                    <optional>true</optional>
                </attribute>
                <attribute name="default-x">
                    <optional>true</optional>
                </attribute>
                <attribute name="default-y">
                    <optional>true</optional>
                </attribute>
            </attributes>
        </element>
    
        <element name="source">
            <optional>true</optional>
        </element>

        <element name="staff-distance">
            <tolerance-ratio>0.02</tolerance-ratio>
        </element>
    
        <element name="stem">
            <attributes>
                <attribute name="default-y">
                    <optional>true</optional>
                </attribute>
            </attributes>
        </element>

        <element name="system-distance">
            <tolerance-ratio>0.02</tolerance-ratio>
        </element>

        <element name="top-system-distance">
            <tolerance-ratio>0.02</tolerance-ratio>
        </element>

        <element name="volume">
            <tolerance-ratio>0.05</tolerance-ratio>        
        </element>

        <element name="width">
            <tolerance-ratio>0.05</tolerance-ratio>        
        </element>
        
    </elements>
    
</filter>
