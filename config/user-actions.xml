     This file gathers the user-addedclasses to be loaded as UI actions.
     
     The order of menus in the menu bar are predefined, but the order of items
     within a section depends on this.
     
     Version $Id$
     -->
     
<actions>
    
    <!-- Just as an example -->
    <action domain="DEBUG" section="20" class="omr.ui.GuiActions" method="dumpEventServices"/>
